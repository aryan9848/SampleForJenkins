<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>servlet</servlet-name>
    <servlet-class>newuser.registering</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servlet1</servlet-name>
    <servlet-class>newuser.displaying</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet1</servlet-name>
    <url-pattern>/disi</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>servlet2</servlet-name>
    <servlet-class>newuser.registrations</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet2</servlet-name>
    <url-pattern>/regis</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>filter-registering</filter-name>
    <filter-class>newuser.filter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>filter-registering</filter-name>
    <url-pattern>/regis</url-pattern>
  </filter-mapping>
</web-app>
